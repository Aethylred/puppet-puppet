        "12.04",
        "14.04",
        "16.04"
      ]
    }
   ],
  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">=3.2.0 <5.0.0" },
    { "name": "puppetlabs/concat", "version_requirement": ">=2.2.0" }
